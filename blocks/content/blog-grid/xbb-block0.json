{
  "name": "xbb/content/blog-grid",
  "title": "Content Blog Grid",
  "category": "content",
  "template": "block.tpl",
  "props": {
    "posts": {
      "type": "array",
      "default": []
    },
    "title": {
      "type": "string",
      "default": "Latest posts"
    },
    "subtitle": {
      "type": "string",
      "default": "What\u2019s new"
    },
    "limit": {
      "type": "number",
      "default": 6
    },
    "browse_url": {
      "type": "string",
      "default": "#"
    }
  },
  "providers": [
    "Xbb\\Blocks\\Provider\\MockBlogGridProvider"
  ]
}
