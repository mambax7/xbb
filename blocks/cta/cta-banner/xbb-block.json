{
  "name": "xbb/cta/cta-banner",
  "title": "CTA \u00b7 Banner",
  "category": "cta",
  "template": "block.tpl",
  "props": {
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "variant": {
      "type": "enum",
      "values": [
        "default",
        "primary",
        "secondary",
        "contrast"
      ],
      "default": "default"
    },
    "align": {
      "type": "enum",
      "values": [
        "",
        "start",
        "center",
        "end"
      ],
      "default": ""
    },
    "bg": {
      "type": "enum",
      "values": [
        "",
        "light",
        "dark",
        "muted"
      ],
      "default": ""
    },
    "id": {
      "type": "string",
      "default": ""
    },
    "class": {
      "type": "string",
      "default": ""
    },
    "button_text": {
      "type": "string",
      "default": "Start now"
    }
  }
}

